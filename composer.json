{
  "name": "woocommerce/google-listings-and-ads",
  "type": "wordpress-plugin",
  "description": "Native integration with Google that allows merchants to easily display their products across Googleâ€™s network.",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.3",
    "automattic/jetpack-autoloader": "^2.4",
    "automattic/jetpack-config": "^1.4",
    "automattic/jetpack-connection": "^1.20",
    "google/apiclient": "^2.7",
    "googleads/google-ads-php": "^8.0",
    "league/container": "^3.3",
    "league/iso3166": "^3.0",
    "psr/container": "^1.0",
    "symfony/validator": "^5.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7",
    "wp-coding-standards/wpcs": "^2.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^v0.7",
    "pfrenssen/phpcs-pre-push": "1.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "pfrenssen/phpcs-pre-push",
        "version": "1.0",
        "source": {
          "url": "https://gist.github.com/498fc52fea3f965f6640.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "license": "GPL-3.0",
  "autoload": {
    "psr-4": {
      "Automattic\\WooCommerce\\GoogleListingsAndAds\\": "src/",
      "Automattic\\WooCommerce\\GoogleListingsAndAds\\Vendor\\League\\Container\\": "vendor/league/container/src/",
      "Automattic\\WooCommerce\\GoogleListingsAndAds\\Vendor\\League\\ISO3166\\": "vendor/league/iso3166/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Automattic\\WooCommerce\\GoogleListingsAndAds\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.27"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "Google\\Task\\Composer::cleanup",
      "php ./bin/prefix-vendor-namespace.php",
      "bash ./bin/googleads-remove-older-versions.sh",
      "bash ./bin/add-pre-push-hook.sh"
    ],
    "post-update-cmd": [
      "Google\\Task\\Composer::cleanup",
      "php ./bin/prefix-vendor-namespace.php",
      "bash ./bin/googleads-remove-older-versions.sh",
      "bash ./bin/add-pre-push-hook.sh"
    ]
  },
  "archive": {
    "exclude": [
      "!/js/build",
      "!/vendor/*"
    ]
  },
  "extra": {
    "google/apiclient-services": [
      "ShoppingContent",
      "SiteVerification"
    ]
  }
}
